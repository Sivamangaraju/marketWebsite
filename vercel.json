{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "includeFiles": [
                    "market/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ],
    "scripts": {
        "start": "gunicorn -w 4 -b 0.0.0.0:8000 run:app"
    }
}